            <TextBlock Height="143" HorizontalAlignment="Left" Margin="0,311,0,0" Name="adviceBox" Text="Advice" VerticalAlignment="Top" Width="558" />
            <Canvas Height="296" HorizontalAlignment="Left" Margin="225,8,0,0" Name="canvas1" VerticalAlignment="Top" Width="329" />
