        <CheckBox Content="Audio Cues" Height="16" HorizontalAlignment="Left" Margin="675,556,0,0" Name="audioCues" VerticalAlignment="Top" />
        <Grid Height="454" HorizontalAlignment="Left" Margin="198,70,0,0" Name="quickLiftGrid" VerticalAlignment="Top" Width="558">
