        <Button Content="Overhead Press" Height="23" HorizontalAlignment="Left" Margin="319,181,0,0" Name="overheadPressButton" VerticalAlignment="Top" Width="100" Click="exerciseButtonClick" />
        <Button Content="Hang Clean" Height="23" HorizontalAlignment="Left" Margin="319,254,0,0" Name="hangCleanButton" VerticalAlignment="Top" Width="100" Click="exerciseButtonClick" />
        <Label Content="Select an Exercise" Height="42" HorizontalAlignment="Left" Margin="158,31,0,0" Name="label1" VerticalAlignment="Top" FontSize="22" />
        <Rectangle Height="41" HorizontalAlignment="Left" Margin="151,31,0,0" Name="rectangle1" Stroke="Black" VerticalAlignment="Top" Width="190" />
        <Button Content="System Information" Height="23" HorizontalAlignment="Left" Margin="183,335,0,0" Name="systemInfo" VerticalAlignment="Top" Width="123" Click="systemInfo_Click" />
    </Grid>
